# Database Configuration
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# OpenAI Configuration
OPENAI_API_KEY=sk-your_openai_api_key_here
MONTHLY_BUDGET_LIMIT=20.0
DALL_E_MODEL=dall-e-3
DALL_E_QUALITY=standard
DALL_E_SIZE=1024x1024

# Bot Detection APIs
FINGERPRINTJS_API_KEY=your_fingerprintjs_api_key
FINGERPRINTJS_SECRET_KEY=your_fingerprintjs_secret_key
IPHUB_API_KEY=your_iphub_api_key
BOT_DETECTION_ENABLED=true
FINGERPRINT_CONFIDENCE_THRESHOLD=0.8

# Webhook Configuration
WEBHOOK_URL=https://your-app.com/api/webhook/behavior-score
WEBHOOK_MAX_RETRIES=3
WEBHOOK_RETRY_DELAY=2
WEBHOOK_TIMEOUT=30

# Security Keys
JWT_SECRET_KEY=your_jwt_secret_key_for_sessions
ENCRYPTION_KEY=your_32_character_encryption_key_here
API_SECRET_KEY=your_api_secret_for_internal_auth

# Email/Notification Services
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password
ADMIN_EMAIL=admin@yourcompany.com

# Slack Integration (for alerts)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your/slack/webhook
SLACK_CHANNEL=#security-alerts

# Discord Integration (for alerts)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your/discord/webhook

# Cache Configuration
REDIS_URL=redis://localhost:6379/0
CACHE_TTL_SECONDS=300

# Rate Limiting
RATE_LIMIT_ENABLED=true
MAX_REQUESTS_PER_HOUR=1000
MAX_CONCURRENT_REQUESTS=10

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=./logs/bse_system.log
AUDIT_LOG_RETENTION_DAYS=365

# Development/Testing
TESTING_MODE=false
DEBUG_MODE=false
MIGRATE_ENABLED=false

# Dashboard Configuration
DASHBOARD_PORT=5002
DASHBOARD_REFRESH_INTERVAL=30
DASHBOARD_MAX_LEADERBOARD_USERS=50

# Feature Flags
MEME_GENERATION_ENABLED=true
REAL_TIME_ANALYTICS_ENABLED=true
ADVANCED_BOT_DETECTION_ENABLED=true
FAKE_REFERRAL_DETECTION_ENABLED=true
